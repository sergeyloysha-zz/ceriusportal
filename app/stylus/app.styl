@import '_normalize'

font-base = 'Roboto', Helvetica, Arial, sans-serif

size-font = 1.4rem
size-line = 2rem

size-s = 768px
size-m = 980px
size-l = 1200px

color-white = #ffffff
color-main = #4c4d4f
color-background = #dee2e4
color-blue = #1c6acc
color-green = #81c104

mediaMin(minWidth)
  @media screen and (min-width: minWidth)
    {block}
mediaBetween(minWidth, maxWidth)
  @media screen and (min-width: minWidth) and (max-width: maxWidth)
    {block}

clearfix()
  zoom 1
  &:after
  &:before
    content ""
    display table
  &:after
    clear both

*
  &
  &:before
  &:after
    box-sizing border-box
  &:active
    -webkit-tap-highlight-color: alpha(color-white, 0)

::selection
  background alpha(#28bbbb, 0.05)

input
textarea
  &:focus
    outline 0

html
  font-size 62.5%
  text-rendering optimizeLegibility
  -webkit-font-smoothing antialiased
  -moz-osx-font-smoothing grayscale
  background color-background

body
  font 400 size-font/size-line font-base
  color color-main

strong
  font-weight 700

img
  max-width 100%
  height auto

a
a:link
a:active
a:visited
  text-decoration none
  cursor pointer

.btn
  display inline-block
  border 0
  border-radius 5rem
  padding 1rem 2rem
  background color-green
  font-size 1.6rem
  color color-white

.container
  width 100%
  max-width size-l
  min-width size-s
  margin 0 auto
  padding 0 1.5rem
  clearfix()

  &__side
    width 28rem
    margin-right 4rem
    float left

  &__content
    margin-left 32rem

.header
  background color-white
  padding 1rem 0
  border-bottom 1px solid #d3d3d3

  &__content
    display flex
    align-items center
    justify-content space-between
    min-height 5.8rem

  &__nav
    margin-left -2rem

  &__contacts
    &-list
      margin 0
      padding 0
      list-style none
      clearfix()

    &-item
      float left
      margin-right 2rem

      &:last-child
        margin-right 0

    &-link
      font-size 1.6rem
      color color-blue

.nav
  &__list
    margin 0
    padding 0
    list-style none
    clearfix()

  &__item
    float left
    margin 0 1rem

  &__link
    display block
    padding 1rem
    color #221f1f
    font-weight 500
    transition color .15s ease-out

    &_active
    &:hover
      color color-blue

.logo
  display block
  width 12.1rem
  height 5.8rem
  background url('data:image/png;base64,')
  background-repeat no-repeat
  background-position center

.wrapper
  padding 4rem 0

.side
  background color-white
  overflow hidden

  &-block
    position relative

    &__title
      position relative
      padding 2rem 3rem
      font-size 1.8rem
      color #3c4144
      box-shadow 0 0 4rem rgba(0,0,0,.15)
      z-index 2

    &__content
      position relative
      padding 3rem
      background #f9f9f9
      z-index 1

      &_white
        background #fff

    &__list
      margin 0
      padding 0
      list-style none

      &-item
        margin-bottom 1rem

        &:last-child
          margin-bottom 0

      &-label
        position relative

      &-checkbox
        opacity 0
        z-index -1
        position absolute

      &-link
        position relative
        display inline-block
        color #000000
        margin-left 2.5rem

        &:before
          position absolute
          left -2.5rem
          top 0.5rem
          content ' '
          width 1rem
          height 1rem
          background-image url('data:image/svg+xml;base64,')
          background-repeat no-repeat
          background-position center
          background-size 1rem

.footer
  background #b0b9bd
  padding 2rem 0

  &__content
    display flex
    align-items center
    min-height 5.8rem

  &__copyright
    color #000000

  &__link
    font-weight 700
    color #000000

.create-post
  display flex
  justify-content center
  align-items center
  background #1781c1
  padding 3rem

  &__text
    font-size 3rem
    font-weight 300
    color color-white
    line-height 3.4rem
    margin 0

  &__btn
    text-transform uppercase
    margin-left 5rem

.executives
  &__find
    margin 3rem 0
    text-align center

    &-text
      font-size 2rem
      line-height 2.4rem
      color #3c4144

    &-link
      color #1781c1
      border-bottom 1px solid transparent
      transition border-bottom .15s ease-out

      &:hover
        border-bottom 1px solid #1781c1

  &__list
    margin 0
    padding 0 1.5rem
    list-style none
    background color-white
    box-shadow 0 0 1rem rgba(0,0,0,.1)

    &-item
      border-bottom 1px solid #dfdfdf
      padding 2rem 3rem

      &:last-child
        border-bottom none

.executive
  clearfix()

  &__id
    font-size 1.6rem
    font-weight 700
    color #818485
    margin-bottom 2rem

  &__left
    width 30%
    float left
    padding-right 2rem

  &__right
    width 70%
    float left

  &__link
    color #1781c1
    border-bottom 1px solid transparent
    transition border-bottom .15s ease-out

    &:hover
      border-bottom 1px solid alpha(#1781c1, .3)

  &__lists
    padding 1.5rem

    &_expertise
      background #f7f8f8

    &-title
      font-weight 500
      color #3c4144

    &-text
      color #6a6a6a
